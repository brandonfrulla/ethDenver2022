{
    "manifest_version": 2,
    "name": "Leap (Watch To Earn!)",
    "description": "Leap tracks your watchtime for spcific streamers and reward you with tokens accordingly",
    "version": "0.1",
    "key": "",
    "author": "Leap Foundation",
    "icons": { 
        "16": "/images/leap_logo_16.png",
        "48": "/images/leap_logo_48.png",
        "128": "/images/leap_logo_128.png" 
    },
    "background": {
        "scripts": [
            "./js/background.js",
            "./js/jsrsasign-all-min.js"
        ]
      },
    "browser_action": {
        "default_icon": "/images/leap_logo_128.png",
        "default_popup": "index.html",
        "default_title": "Leap (Watch To Earn)"
    },
    "permissions": [
        "identity",
        "tabs",
        "https://www.youtube.com/watch*",
        "https://www.twitch.tv/"
    ]
}